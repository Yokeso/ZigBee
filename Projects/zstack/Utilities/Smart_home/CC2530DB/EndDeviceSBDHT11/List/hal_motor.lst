     98                   }                                         \
     99                   else {                                    \
    100                       IO_FUNC_PORT_PIN(1,4,IO_FUNC_PERIPH); \
    101                   }                                         \
    102                }                                            \
    103                else                                         \
    104                {                                            \
    105                   if(PERCFG & 0x10) {                       \
    106                      IO_FUNC_PORT_PIN(2,3,IO_FUNC_PERIPH);  \
    107                   }                                         \
    108                  else {                                     \
    109                     IO_FUNC_PORT_PIN(1,1,IO_FUNC_PERIPH);   \
    110                  }                                          \
    111                } )
