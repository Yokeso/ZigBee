     89          
     90          
     91          // Macro for setting timer 3 or 4 as a capture timer
     92          #define TIMER34_CAPTURE_TIMER(timer,edge)          \
     93             st(                                             \
     94                T##timer##CCTL1 = edge;                      \
     95                if(timer == 3){                              \
     96                   if(PERCFG & 0x20) {                       \
