    721          void HalLcd_HW_WriteLine(uint8 line, const char *pText)
    722          {
    723              uint8 count;
    724              uint8 totalLength = (uint8)osal_strlen( (char *)pText );
    725            
    726              /* Write the content first */
    727              for (count = 0; count < totalLength; count++)
    728              {
    729                  HalLcd_HW_WriteChar(line, (count + 1), (*(pText++)));
    730              }
    731            
    732              /* Write blank spaces to rest of the line */
    733              for (count = totalLength; count < LCD_MAX_LINE_LENGTH; count++)
    734              {
    735                 HalLcd_HW_WriteChar(line, (count + 1), ' ');
    736              }
    737          }
