    751              {
    752                  // 32 NOPs == 1 usecs
    753                  asm("nop"); asm("nop"); asm("nop"); asm("nop"); asm("nop");
    754                  asm("nop"); asm("nop"); asm("nop"); asm("nop"); asm("nop");
