    485           **************************************************************************************************/
    486          void HalLcd_HW_Control(uint8 cmd)
    487          {
    488              uint8 i;
    489              
    490              LCD_ACTIVATE_CS();
    491              LCD_RS_CMD();
    492              
    493              for (i = 0; i < 8; i++) 
    494              { 
    495                  LCD_SCLK_L; 
    496                  if (cmd & 0x80) 
    497                  {
    498                      LCD_SDA_H;
    499                  }
    500                  else 
    501                  {
    502                      LCD_SDA_L;
    503                  }
    504                  
    505                  LCD_SCLK_H; 
    506                  cmd <<= 1; 
    507              }
    508              // LCD_RELEASE_CS();
    509          }
