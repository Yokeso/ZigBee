    423           * @fn      Smart_home_ProcessZDOMsgs()
    424           *
    425           * @brief   Process response messages
    426           *
    427           * @param   none
    428           *
    429           * @return  none
    430           */
    431          static void Smart_home_ProcessZDOMsgs( zdoIncomingMsg_t *inMsg )
    432          {
    433            switch ( inMsg->clusterID )
    434            {
    435              case End_Device_Bind_rsp:
    436                if ( ZDO_ParseBindRsp( inMsg ) == ZSuccess )
    437                {
    438                  // Light LED
    439                  HalLedSet( HAL_LED_4, HAL_LED_MODE_ON );
    440                }
    441          #if defined(BLINK_LEDS)
    442                else
    443                {
    444                  // Flash LED to show failure
    445                  HalLedSet ( HAL_LED_4, HAL_LED_MODE_FLASH );
    446                }
    447          #endif
    448                break;
    449                
    450              case Match_Desc_rsp:
    451                {
    452                  ZDO_ActiveEndpointRsp_t *pRsp = ZDO_ParseEPListRsp( inMsg );
    453                  if ( pRsp )
    454                  {
    455                    if ( pRsp->status == ZSuccess && pRsp->cnt )
    456                    {
    457                      /*
    458                      Smart_home_TxAddr.addrMode = (afAddrMode_t)Addr16Bit;
    459                      Smart_home_TxAddr.addr.shortAddr = pRsp->nwkAddr;
    460                      // Take the first endpoint, Can be changed to search through endpoints
    461                      Smart_home_TxAddr.endPoint = pRsp->epList[0];
    462                      */
    463                      
    464                      // Light LED
    465                      HalLedSet( HAL_LED_4, HAL_LED_MODE_ON );
    466                    }
    467                    osal_mem_free( pRsp );
    468                  }
    469                }
    470                break;
    471            }
    472          }
