    428                {
    429                  // Light LED
    430                  HalLedSet( HAL_LED_4, HAL_LED_MODE_ON );
    431                }
    432          #if defined(BLINK_LEDS)
    433                else
    434                {
    435                  // Flash LED to show failure
    436                  HalLedSet ( HAL_LED_4, HAL_LED_MODE_FLASH );
    437                }
    438          #endif
    439                break;
    440                
    441              case Match_Desc_rsp:
    442                {
    443                  ZDO_ActiveEndpointRsp_t *pRsp = ZDO_ParseEPListRsp( inMsg );
    444                  if ( pRsp )
    445                  {
    446                    if ( pRsp->status == ZSuccess && pRsp->cnt )
    447                    {
    448                      /*
    449                      Smart_home_TxAddr.addrMode = (afAddrMode_t)Addr16Bit;
    450                      Smart_home_TxAddr.addr.shortAddr = pRsp->nwkAddr;
    451                      // Take the first endpoint, Can be changed to search through endpoints
    452                      Smart_home_TxAddr.endPoint = pRsp->epList[0];
    453                      */
    454                      
    455                      // Light LED
    456                      HalLedSet( HAL_LED_4, HAL_LED_MODE_ON );
    457                    }
    458                    osal_mem_free( pRsp );
    459                  }
    460                }
    461                break;
    462            }
    463          }
