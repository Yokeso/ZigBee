    448          #endif
    449                break;
    450                
    451              case Match_Desc_rsp:
    452                {
    453                  ZDO_ActiveEndpointRsp_t *pRsp = ZDO_ParseEPListRsp( inMsg );
    454                  if ( pRsp )
    455                  {
    456                    if ( pRsp->status == ZSuccess && pRsp->cnt )
    457                    {
    458                      /*
    459                      Smart_home_TxAddr.addrMode = (afAddrMode_t)Addr16Bit;
    460                      Smart_home_TxAddr.addr.shortAddr = pRsp->nwkAddr;
    461                      // Take the first endpoint, Can be changed to search through endpoints
    462                      Smart_home_TxAddr.endPoint = pRsp->epList[0];
    463                      */
    464                      
    465                      // Light LED
    466                      HalLedSet( HAL_LED_4, HAL_LED_MODE_ON );
    467                    }
    468                    osal_mem_free( pRsp );
    469                  }
    470                }
    471                break;
    472            }
    473          }
