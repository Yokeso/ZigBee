    411           * @brief       Helper function for ZDO_UpdateNwkStatus.
    412           *
    413           * input parameters
    414           *
    415           * @param       taskId - The OSAL task identifier to which to send the ZDO_STATE_CHANGE_EVT.
    416           * @param       state - The current device state.
    417           *
    418           * output parameters
    419           *
    420           * None.
    421           *
    422           * @return      None.
    423           **************************************************************************************************
    424           */
    425          static void zdoSendStateChangeMsg(uint8 state, uint8 taskId)
    426          {
    427            osal_event_hdr_t *pMsg = (osal_event_hdr_t *)osal_msg_find(taskId, ZDO_STATE_CHANGE);
    428          
    429            if (NULL == pMsg)
    430            {
    431              if (NULL == (pMsg = (osal_event_hdr_t *)osal_msg_allocate(sizeof(osal_event_hdr_t))))
    432              {
    433                // Upon failure to notify any EndPoint of the state change, re-set the ZDO event to
    434                // try again later when more Heap may be available.
    435                osal_set_event(ZDAppTaskID, ZDO_STATE_CHANGE_EVT);
    436              }
    437              else
    438              {
    439                pMsg->event = ZDO_STATE_CHANGE;
    440                pMsg->status = state;
    441          
    442                (void)osal_msg_send(taskId, (uint8 *)pMsg);
    443              }
    444            }
    445            else
    446            {
    447              // Modify in place the status of an existing ZDO_STATE_CHANGE message to the EndPoint.
    448              pMsg->status = state;
    449            }
    450          }
