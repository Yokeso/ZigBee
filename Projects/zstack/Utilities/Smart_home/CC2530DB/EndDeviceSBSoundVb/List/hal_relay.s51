        MOV     DPTR,#relayStatus
        MOVX    A,@DPTR
        ANL     A,#0xf
        ORL     A,#0x10
        SJMP    ??HalRelayCtl_6
//   79         break;
//   80       }
//   81     case 0x20: // 打开
//   82       {
//   83         RELAY_MODULE_2_PIN =  RELAY_SWITCH_ON;
??HalRelayCtl_4:
